<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.developers.quickcollect.acercade">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:src="@drawable/logo2"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Versión 0.1"
        android:id="@+id/textView5"
        android:layout_below="@+id/imageView2"
        android:layout_centerHorizontal="true"
        android:textSize="40dp"
        android:textColor="#ffffa000" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Aplicación desarrollada en android "
        android:id="@+id/textView6"
        android:layout_below="@+id/textView5"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Desarradores acargo "
        android:id="@+id/textView7"
        android:textAlignment="center"
        android:layout_marginTop="37dp"
        android:layout_below="@+id/textView6"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Omar Torres Flores. "
        android:id="@+id/textView8"
        android:textSize="22dp"
        android:layout_below="@+id/textView9"
        android:layout_centerHorizontal="true"
        android:textColor="#ff0c50ff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Eduardo Izquierdo Rojas."
        android:id="@+id/textView9"
        android:textSize="22dp"
        android:layout_below="@+id/textView7"
        android:layout_centerHorizontal="true"
        android:textColor="#ff0c50ff" />
</RelativeLayout>
